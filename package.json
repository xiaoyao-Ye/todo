{
  "name": "todo",
  "version": "0.0.3",
  "main": "dist-electron/main/index.js",
  "description": "Ghosteye todo.",
  "author": "Ghosteye <yqcyecong@163.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "scripts": {
    "test": "vitest run",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder build",
    "preview": "vite preview",
    "api": "initapi create",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "lint": "prettier --write **/*.{vue,ts,js,md}",
    "commit": "git-cz",
    "commit:origin": "git pull && git add . && git-cz && git push"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.20",
    "@unocss/preset-icons": "^0.55.3",
    "@unocss/preset-uno": "^0.55.3",
    "@unocss/reset": "^0.55.3",
    "@vitejs/plugin-vue": "^4.3.3",
    "dayjs": "^1.11.10",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3",
    "git-cz": "^4.9.0",
    "initapi": "^0.6.0",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.1.6",
    "unocss": "^0.55.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.13.0-beta.3",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md,vue}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
